{
  "name": "warlof/seat-connector",
  "description": "Provide basic layer for SeAT connectors.",
  "type": "seat-plugin",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Warlof Tutsimo",
      "email": "loic.leuilliot@gmail.com"
    },
    {
      "name": "Zenobio93",
      "email": "development@hellmund.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Warlof\\Seat\\Connector\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "eveseat/services": "^5.0",
    "eveseat/eveapi": "^5.0",
    "eveseat/web": "^5.0",
    "eveseat/api": "^5.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Warlof\\Seat\\Connector\\SeatConnectorServiceProvider"
      ]
    }
  },
  "require-dev": {
    "rector/rector": "^0.18.2",
    "driftingly/rector-laravel": "^0.24.1"
  }
}
